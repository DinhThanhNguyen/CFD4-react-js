Users/dinhthanh/Desktop/CFD/ReactJS/CFD4-React-JS/src/components/CourseItem.js",["263"],"import React from 'react'\nimport { Link } from 'react-router-dom'\n\nexport default function CourseItem(props) {\n    return (\n        <div className=\"col-md-4 course\">\n                <div className=\"wrap\">\n                    <Link className=\"cover\" to={`/chi-tiet-khoa-hoc/${props.slug}`}>\n                        <img src={props.thumbnail.link} alt=\"\" />\n                        <span className=\"badge b1\">{props.status}</span>\n                        <div className=\"hover\">\n                            <div className=\"top\">\n                                <div className=\"user\">\n                                    <img src=\"/img/icon-user-white.svg\" alt=\"\" />\n                          12</div>\n                                <div className=\"heart\">\n                                    <img src=\"/img/icon-heart.svg\" alt=\"\" /> 100\n                        </div>\n                            </div>\n                            <div className=\"share\">\n                                <img src=\"/img/icon-viewmore.svg\" alt=\"\" />\n                            </div>\n                        </div>\n                    </Link>\n                    <div className=\"info\" to={`/chi-tiet-khoa-hoc/${props.slug}`}>\n                        <a className=\"name\" href=\"#\">\n                           {props.title}\n                    </a>\n                        <p className=\"des\">\n                            {props.short_description}\n                    </p>\n                    </div>\n                    <div className=\"bottom\">\n                        <div className=\"teacher\">\n                            <div className=\"avatar\">\n                                <img src={props.teacher.avatar?.thumbnail?.['thumbnail-1'] || props.teacher.avatar.link} alt=\"\" />\n                            </div>\n                            <div className=\"name\">{props.teacher.title}</div>\n                        </div>\n                        <Link to={`/dang-ky/${props.slug}`} className=\"register-btn\">Đăng Ký</Link>\n                    </div>\n                </div>\n            </div>\n    )\n}\n","/Users/dinhthanh/Desktop/CFD/ReactJS/CFD4-React-JS/src/pages/course-detail/components/Accordion.js",[],"/Users/dinhthanh/Desktop/CFD/ReactJS/CFD4-React-JS/src/pages/profile/Components/CourseItem.js",["264"],"import React from 'react'\n\nexport default function CourseItem({ course, trang_thai }) {\n\n    let now = new Date();\n\n    let start = new Date(course.opening_time)\n\n    let temp2 = start.getTime()\n\n    start.setMonth(start.getMonth() + 2)\n\n    temp2 = start.getTime() - temp2\n\n    temp2 = Math.round(temp2 / 1000)\n\n    let temp = now.getTime() - start.getTime()\n\n    temp = Math.round(temp / 1000)\n\n    let process = temp > 0 ? (temp / temp2) * 100 : 100;\n\n\n    return (\n        <div className=\"item\">\n            <div className=\"cover\">\n                <img src={course.thumbnail?.thumbnail?.['thumbnail-1'] || course.thumbnail.link} alt=\"\" />\n            </div>\n            <div className=\"info\">\n                <a href=\"#\" className=\"name\">{course.title}</a>\n                <div className=\"date\">Khai giảng ngày {course.opening_time}</div>\n                <div className=\"row\">\n                    <div className>\n                        <img src=\"/img/clock.svg\" alt=\"\" className=\"icon\" />{54} giờ\n                        </div>\n                    <div className>\n                        <img src=\"/img/play.svg\" alt=\"\" className=\"icon\" />{course.count_video}\n                    </div>\n                    <div className>\n                        <img src=\"/img/user.svg\" alt=\"\" className=\"icon\" />{20} học viên\n                        </div>\n                </div>\n                <div className=\"process\">\n                    <div className