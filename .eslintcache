components/RelatedCourses'\n\nexport default function CourseDetail() {\n\n    let [detail, setDetail] = useState()\n    useEffect(() => {\n        fetch('http://cfd-reactjs.herokuapp.com/elearning/v4/course/cfd5-react-js')\n        .then(res => res.json())\n         .then(res => {\n             console.log(res)\n             setDetail(res.data)\n         })\n    }, [])\n\n    return (\n        <main className=\"course-detail\" id=\"main\">\n            <Banner {...detail} />\n            <IntroCourse />\n            <MemberProject />\n            <RelatedCourses />\n      </main>\n    )\n}\n","/Users/dinhthanh/Desktop/CFD/ReactJS/CFD4-React-JS/src/pages/khoa-hoc/index.js",[],"/Users/dinhthanh/Desktop/CFD/ReactJS/CFD4-React-JS/src/pages/project/index.js",["233","234","235","236","237","238","239","240"],"import React from 'react'\n\nexport default function Project() {\n    return (\n<main className=\"projectpage\" id=\"main\">\n        <section className=\"section-1\">\n          <div className=\"container\">\n            <h2 className=\"main-title\">dự án</h2>\n            <p className=\"top-des\">\n              Lorem ipsum dolor sit amet consectetur adipiscing elit. aliquam tincidunt elementum sem non\n              luctus\n            </p>\n            <div className=\"list row\">\n              <a href=\"#\" className=\"item col-md-6\">\n                <div className=\"wrap\">\n                  <div className=\"cover\">\n                    <img src=\"/img/project/project (1).jpg\" alt=\"\" />\n                  </div>\n                  <div className=\"info\">\n                    <div className=\"name\">furnitown</div>\n                    <div className=\"makeby\">Hoàn thành với Trần Nghĩa </div>\n                  </div>\n                </div>\n              </a>\n              <a href=\"#\" className=\"item col-md-6\">\n                <div className=\"wrap\">\n                  <div className=\"cover\">\n                    <img src=\"/img/project/proj